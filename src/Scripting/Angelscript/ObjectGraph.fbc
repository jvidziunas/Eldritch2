file_identifier	"ESOG";
file_extension	"e2serializedobjectgraph";

namespace Eldritch2.Scripting.AngelScript.FlatBuffers;

struct ObjectReference {
	ulong index;
}

union ObjectProperty {
	long,
	ulong,
	double,
	ObjectReference
}

table ObjectDefinition {
	Properties	: [ObjectProperty];
}

table ObjectGraph {
	Objects		: [ObjectDefinition];
}

root_type ObjectGraph;