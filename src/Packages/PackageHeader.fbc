file_identifier	"ECRF";
file_extension	"PackageHeader";

namespace Eldritch2.FileSystem.FlatBuffers;

struct BlockDescriptor {
	offset	: ulong;
	length	: ulong;
}

table Export {
	name	: string (required);
	type	: string (required);
	block	: BlockDescriptor (required);
}

table Header {
	imports : [string];
	exports : [Export];
}

root_type Header;