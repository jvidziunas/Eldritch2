namespace Eldritch2.FlatBuffers;

struct Float2 {
	X	: float;
	Y	: float;
}

struct Float3 {
	X	: float;
	Y	: float;
	Z	: float;
}

struct Quaternion {
	I	: float;
	J	: float;
	K	: float;
	W	: float;
}

struct CompressedUnitQuaternion {
	Bits	: uint;
}

struct Transformation {
	Translation : Float3;
	Orientation : CompressedUnitQuaternion;
}