namespace flatbuffers;

struct TransientObjectMarker {
	Dummy : ubyte;
}

struct Float2 {
	X	: float;
	Y	: float;
}

struct Float3 {
	X	: float;
	Y	: float;
	Z	: float;
}

struct Quaternion {
	I	: float;
	J	: float;
	K	: float;
	W	: float;
}

struct UnitQuaternion {
	Bits : uint;
}

struct Transformation {
	Translation : Float3;
	Orientation : UnitQuaternion;
}