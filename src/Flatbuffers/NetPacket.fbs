namespace Eldritch2.Core.FlatBuffers;

table AttachPlayer {
	Id       : ulong;
	GameData : [ubyte] (flexbuffer);
}

struct DetachPlayer {
	Id : ulong;
}

struct AckMessage {
	Id : ulong;
}

union NetMessage {
	AttachPlayer,
	DetachPlayer,
	AckMessage,
}

table NetPacket {
	Messages : [NetMessage];
}

root_type NetPacket;